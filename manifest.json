{
  "name": "ip-x-x-x-x.ec2.internal to IPs",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Converts all the ip-x-x-x-x.ec2.internal to it's IP format",
  "homepage_url": "https://github.com/ashwanthkumar/chrome-ec2.internal",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/link-changer.js"
      ]
    }
  ]
}
